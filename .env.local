# JWT Secret - Use a strong 32+ character secret
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-make-it-long-and-random

# Google Gemini API Key
GOOGLE_GENERATIVE_AI_API_KEY=your-actual-gemini-api-key-here

# Vercel KV Database (REQUIRED for auth to work)
KV_URL=your-vercel-kv-url
KV_REST_API_URL=your-vercel-kv-rest-api-url
KV_REST_API_TOKEN=your-vercel-kv-rest-api-token
KV_REST_API_READ_ONLY_TOKEN=your-vercel-kv-rest-api-read-only-token

# App URL (update for production)
NEXT_PUBLIC_APP_URL=https://your-vercel-app-url.vercel.app

# Node Environment
NODE_ENV=production
